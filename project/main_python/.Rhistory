comp <- cbind(ring, vib, B.L, dip, nbo, ste, dih, pol)
} else {
if (dih_answer == "n") {
comp <- cbind(ring, vib, B.L, dip, nbo, ste, pol)
}
}
row.names(comp) <- molecules
for (i in 1:length(comp)) {
if (class(comp[[i]]) == "character") {
comp[[i]] <- as.numeric(comp[[i]])
}
}
return(comp)
}
